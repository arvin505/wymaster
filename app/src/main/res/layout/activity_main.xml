<com.miqtech.master.client.view.SlidingMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:wymenu="http://schemas.android.com/apk/res-auto"
    android:id="@+id/id_menu"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_main"
    android:scrollbars="none"
    wymenu:rightPadding="100dp">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:orientation="horizontal">

        <include layout="@layout/layout_menu" />
        <include layout="@layout/activity_main_content" />

    </LinearLayout>


</com.miqtech.master.client.view.SlidingMenu>
