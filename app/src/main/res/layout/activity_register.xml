<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/layout_include_header" />

    <com.miqtech.master.client.view.MyGallery
        android:id="@+id/register_grallery"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:spacing="10dp"></com.miqtech.master.client.view.MyGallery>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="10dp"
        android:background="@color/line" />

    <com.miqtech.master.client.view.MyPagerView
        android:id="@+id/mypagerview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.miqtech.master.client.view.MyPagerView>

</LinearLayout>
